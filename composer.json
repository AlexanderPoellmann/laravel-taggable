{
    "name": "lecturize/laravel-tags",
    "description": "Simple way to tag Eloquent models in Laravel 5.",
    "homepage" : "https://github.com/Lecturize/Laravel-Tags",
    "license": "MIT",
    "keywords": [
        "tags",
        "tagging",
        "taggable",
        "eloquent",
        "model",
        "laravel"
    ],
    "authors": [
        {
            "name"     : "Alexander Manfred Poellmann",
            "homepage" : "http://twitter.com/AMPoellmann",
            "role"     : "developer"
        }
    ],
    "require": {
        "php"                 : ">=5.6.4",
        "doctrine/dbal"       : "2.5.x",
        "illuminate/support"  : ">=5.3",
        "illuminate/database" : ">=5.3",
        "cviebrock/eloquent-sluggable" : "^4.1.0"
    },
    "require-dev": {
        "mockery/mockery" : "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "vendocrat\\Tags\\" : "src/"
        }
    },
    "extra": {
        "branch-alias" : {
            "dev-master" : "0.0.x-dev"
        }
    },
    "support" : {
        "source" : "https://github.com/Lecturize/Laravel-Tags",
        "issues" : "https://github.com/Lecturize/Laravel-Tags/issues"
    }
}
